<h1>Search Results for &ldquo;{{query}}&rdquo;</h1>
{{#hasResults}}
  <table class="search-results">
  <tbody>
{{/hasResults}}
{{#results}}
  <tr>
    <td>{{{icons}}}</td>
  <td>
  <h3>
    <a href="{{url}}">{{name}}</a>
    <span style="font-weight: normal; font-size: smaller">
    {{^show_dev_version}}
      (<a href="{{url}}">{{version}}</a>)
    {{/show_dev_version}}
    {{#show_dev_version}}
      (latest: <a href="{{url}}">{{version}}</a> /
               <a href="{{url}}/versions/{{dev_version_href}}">{{dev_version}}</a>)
    {{/show_dev_version}}
    </span>
  </h3>
  <p>{{desc}}</p>
  <p><small>last updated {{last_uploaded}}</small></p>
  </td>
  </tr>
{{/results}}
{{#hasResults}}
  </tbody>
  </table>
{{/hasResults}}
{{^hasResults}}
  <p>Search for &ldquo;{{query}}&rdquo; did not match any packages.</p>
{{/hasResults}}

{{#search_service}}
  <br/>
  <p style="font-size: smaller">
    You are using our new search backend.
    Please <a href="https://github.com/dart-lang/pub-dartlang-dart/issues/new?labels=Area%3A+search%26title=Search+issue%3A+%3Cdescribe+issue+here%3E%26body=Please+describe+the+issue+and+include+the+search+URL">let us know</a>
    if you encounter issues or unexpected results.
  </p>
{{/search_service}}

{{{pagination}}}
