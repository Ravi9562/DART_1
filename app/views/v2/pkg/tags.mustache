{{#has_tags}}
  {{#wrapper_div}}<div class="tags">{{/wrapper_div}}
  {{#tags}}
    {{#v2_href}}
        <a class="package-tag" href="{{{v2_href}}}">{{text}}</a>
    {{/v2_href}}
    {{^v2_href}}
        <span class="package-tag">{{text}}</span>
    {{/v2_href}}
  {{/tags}}
  {{#wrapper_div}}</div>{{/wrapper_div}}
{{/has_tags}}
