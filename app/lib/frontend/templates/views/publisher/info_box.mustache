{{#has_description}}
<h3 class="title">Description</h3>
<p>{{& description_html}}</p>
{{/has_description}}

{{#website_url}}
<h3 class="title">Website</h3>
<p>
  <a href="{{& website_url}}">{{publisher_id}}</a>
</p>
{{/website_url}}

{{#contact_email}}
<h3 class="title">Contact</h3>
<p>
  <a href="mailto:{{& contact_email}}">{{contact_email}}</a>
</p>
{{/contact_email}}

<h3 class="title">More</h3>
<p>
  <a href="{{& list_packages_search_link}}" rel="nofollow">All packages of {{publisher_id}}</a>
</p>
