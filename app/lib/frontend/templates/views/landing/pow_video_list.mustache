<div class="landing-pow-list">
  {{#videos}}
  <div class="pow-video">
    <a href="{{& video_url }}"
       target="_blank"
       title="{{ description }}">
      <img class="pow-video-thumbnail"
           src="{{& thumbnail_url }}"
           alt="{{ title }}" />
    </a>
  </div>
  {{/videos}}
</div>
