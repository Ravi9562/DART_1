<h4>Analysis</h4>

<p>Last package analysis: <i>{{analysis_status}}</i> on {{date_completed}}.</p>
<p>(Dart: {{dart_sdk_version}}, pana: {{pana_version}}{{#flutter_version}}, Flutter: {{flutter_version}}{{/flutter_version}}).</p>

<h4>Scores</h4>

<ul>
  <li>Health: {{health}}</li>
  <li>Maintenance: {{maintenance}}</li>
  <li>Popularity: {{popularity}}</li>
</ul>

<h4>Platforms</h4>

<p>Detected platforms: {{& platforms_html}}</p>
<blockquote>{{& platforms_reason_html}}</blockquote>

{{#hasSuggestions}}
<h4>Suggestions</h4>
<ul>
  {{#suggestions}}
    <li>
      <div>{{& title}}</div>
      <blockquote>{{& description}}</blockquote>
    </li>
  {{/suggestions}}
</ul>
{{/hasSuggestions}}

{{#has_dependency}}
<h4>Dependencies</h4>
<table class="dependency-table">
  <tr>
    <th>Package</th>
    <th>Constraint</th>
    <th>Resolved</th>
    <th>Available</th>
  </tr>
  {{#dependencies.has_direct}}
  <tr>
    <th colspan="4">Direct dependencies</th>
  </tr>
  {{/dependencies.has_direct}}
  {{#dependencies.direct}}
  <tr>
    <td><a href="/experimental/packages/{{package}}">{{package}}</a></td>
    <td>{{constraint}}</td>
    <td>{{resolved}}</td>
    <td>{{available}}</td>
  </tr>
  {{/dependencies.direct}}
  {{#dependencies.has_transitive}}
  <tr>
    <th colspan="4">Transitive dependencies</th>
  </tr>
  {{/dependencies.has_transitive}}
  {{#dependencies.transitive}}
    <tr>
      <td><a href="/experimental/packages/{{package}}">{{package}}</a></td>
      <td>{{constraint}}</td>
      <td>{{resolved}}</td>
      <td>{{available}}</td>
    </tr>
  {{/dependencies.transitive}}
  {{#dependencies.has_dev}}
  <tr>
    <th colspan="4">Dev dependencies</th>
  </tr>
  {{/dependencies.has_dev}}
  {{#dependencies.dev}}
    <tr>
      <td><a href="/experimental/packages/{{package}}">{{package}}</a></td>
      <td>{{constraint}}</td>
      <td>{{resolved}}</td>
      <td>{{available}}</td>
    </tr>
  {{/dependencies.dev}}
</table>
{{/has_dependency}}
